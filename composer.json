{
    "name": "laravel/homestead",
    "description": "A virtual machine for web artisans.",
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/console": "~2.3|~3.0",
        "symfony/process": "~2.3|~3.0",
        "symfony/yaml": "~2.3|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravel\\Homestead\\": "src/",
            "Localbox\\Installers\\": "src/Installers"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "bin": [
        "bin/homestead"
    ],
    "scripts" : {
        "post-install-cmd": "Localbox\\Installers\\Installer::postInstall",
        "post-update-cmd": "Localbox\\Installers\\Installer::postInstall"
    }
}
